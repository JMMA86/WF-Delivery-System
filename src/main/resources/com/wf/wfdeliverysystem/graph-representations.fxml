<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.wf.wfdeliverysystem.controller.GraphRepresentationController"
      prefHeight="400.0" prefWidth="600.0">
    <HBox VBox.vgrow="ALWAYS" spacing="8">
        <VBox HBox.hgrow="ALWAYS" alignment="TOP_CENTER">
            <Label text="List Representation">
                <font>
                    <Font size="24.0"/>
                </font>
            </Label>
            <TextArea fx:id="listGraphTA" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" editable="false">
                <font>
                    <Font size="14" name="Consolas"/>
                </font>
            </TextArea>
        </VBox>
        <VBox HBox.hgrow="ALWAYS" alignment="TOP_CENTER">
            <Label text="Matrix Representation">
                <font>
                    <Font size="24.0"/>
                </font>
            </Label>
            <TextArea fx:id="matrixGraphTA" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" editable="false">
                <font>
                    <Font size="14" name="Consolas"/>
                </font>
            </TextArea>
        </VBox>
    </HBox>

    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</VBox>
